eval "$(ssh-agent -s)"
ssh-add ~/.ssh/id_ed25519
pbcopy < ~/.ssh/id_ed25519.pub

mkdir studentportal
cd studentportal
git init
vi index.html
index.html

<!doctype html>
<html lang="en">
<head>
<meta charset="utf-8"/>
<meta name="viewport" content="width=device-width,initial-scale=1"/>
<title>Event Registration</title>
</head>
<body>
<h1>Event Registration</h1>
<form id="regForm" action="#" method="post">
<label>Name: <input type="text" name="name" required></label><br>
<label>Email: <input type="email" name="email" required></label><br>
<label>Phone: <input type="tel" name="phone" required></label><br>
<button type="submit">Register</button>
</form>
</body>
</html>

git config user.name "Prithviraj"
git config user.email "prithvirajbmarali@gmail.com"
git add index.html
git commit -m "add"
git branch -M main
git remote set-url/add origin git@github.com:PrithvirajMarali/studentportal.git
